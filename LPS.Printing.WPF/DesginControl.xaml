<UserControl x:Class="LPS.Printing.WPF.DesginControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:local="clr-namespace:LPS.Printing.WPF"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    <Grid>
        <local:DesginCanvas x:Name="DesginCanvas"/>
        <local:AddElementCanvas x:Name="AddElementCanvas"/>
        <local:AddLineCanvas x:Name="AddLineCanvas"/>
        <local:AlignCanvas x:Name="AlignCanvas" IsHitTestVisible="False"/>
    </Grid>
</UserControl>
